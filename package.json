{
  "name": "quicksearch-pino-transport",
  "version": "1.0.3",
  "description": "Pino transport for QuickSearch logging server",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
