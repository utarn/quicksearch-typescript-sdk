    "pino": ">=8.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
